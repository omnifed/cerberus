{
  "name": "@cerberus-design/react",
  "version": "0.0.0",
  "description": "The Cerberus Design React component library",
  "browserslist": "> 0.25%, not dead",
  "sideEffects": false,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "tsup --experimental-dts",
    "clean": "rm -rf ./build",
    "lint:ts": "tsc --project tsconfig.lint.json --noEmit"
  },
  "peerDependencies": {
    "@cerberus-design/panda-preset": "*",
    "@cerberus-design/styled-system": "*",
    "@pandacss/dev": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@cerberus-design/configs": "workspace:*",
    "@cerberus-design/styled-system": "workspace:*",
    "@microsoft/api-extractor": "^7.44.1",
    "@types/react": "^18",
    "react": "^18",
    "react-dom": "^18",
    "tsup": "^8.0.2"
  },
  "publishConfig": {
    "access": "public",
    "types": "build/legacy/index.d.ts",
    "main": "build/legacy/index.cjs",
    "module": "build/legacy/index.js",
    "exports": {
      ".": {
        "import": {
          "types": "./build/modern/index.d.ts",
          "default": "./build/modern/index.js"
        },
        "require": {
          "types": "./build/modern/index.d.cts",
          "default": "./build/modern/index.cjs"
        }
      },
      "./src": {
        "import": {
          "types": "./src/index.d.ts",
          "default": "./src/index.ts"
        }
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        ".": [
          "build/legacy/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "build/**/*",
    "src/**/*",
    "index.ts"
  ],
  "keywords": [
    "panda-css",
    "omni-federal",
    "design-system",
    "css",
    "react",
    "ui",
    "ui library",
    "panda-css react",
    "digital university",
    "du design system"
  ],
  "author": "developers@omnifederal.com",
  "license": "ISC"
}
