{
  "name": "@cerberus-design/react",
  "version": "0.20.0",
  "description": "The Cerberus Design React component library.",
  "browserslist": "> 0.25%, not dead",
  "sideEffects": false,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "prepare": "panda codegen",
    "build": "bun ../../configs/src/bun-build.mjs $(pwd)",
    "deprecated:build": "tsup --experimental-dts noExternals",
    "clean": "rm -rf ./build",
    "lint:ts": "tsc --project tsconfig.lint.json --noEmit"
  },
  "peerDependencies": {
    "@pandacss/dev": "^1.1.0",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@ark-ui/react": "^5.21.0",
    "@dnd-kit/core": "^6.3.1"
  },
  "devDependencies": {
    "@cerberus-design/configs": "workspace:*",
    "@cerberus-design/panda-preset": "workspace:*",
    "@internationalized/date": "^3.8.2",
    "@microsoft/api-extractor": "^7.52.11",
    "@pandacss/dev": "^1.1.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.5.0"
  },
  "publishConfig": {
    "access": "public",
    "types": "build/legacy/index.d.cts",
    "main": "build/legacy/index.cjs",
    "module": "build/modern/index.js",
    "exports": {
      ".": {
        "import": {
          "types": "./build/modern/index.d.ts",
          "default": "./build/modern/index.js"
        },
        "require": {
          "types": "./build/legacy/index.d.cts",
          "default": "./build/legacy/index.cjs"
        }
      },
      "./*": {
        "import": {
          "types": "./build/modern/components/*/index.d.ts",
          "default": "./build/modern/components/*/index.js"
        },
        "require": {
          "types": "./build/legacy/components/*/index.d.cts",
          "default": "./build/legacy/components/*/index.cjs"
        }
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        ".": [
          "build/legacy/index.d.cts"
        ],
        "./*": [
          "build/legacy/components/*/index.d.cts"
        ]
      }
    }
  },
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "keywords": [
    "panda-css",
    "omni-federal",
    "design-system",
    "css",
    "react",
    "ui",
    "ui library",
    "panda-css react",
    "digital university",
    "du design system"
  ],
  "author": "developers@omnifederal.com",
  "license": "ISC"
}
