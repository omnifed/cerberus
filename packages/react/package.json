{
  "name": "@cerberus-design/react",
  "version": "0.0.0",
  "description": "The Cerberus Design React component library.",
  "browserslist": "> 0.25%, not dead",
  "sideEffects": false,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "tsup --experimental-dts noExternals",
    "clean": "rm -rf ./build",
    "lint:ts": "tsc --project tsconfig.lint.json --noEmit"
  },
  "peerDependencies": {
    "@cerberus/icons": "npm:@jsr/cerberus__icons",
    "@cerberus/panda-preset": "npm:@jsr/cerberus__panda-preset",
    "@cerberus/styled-system": "npm:@cerberus-design/styled-system",
    "@pandacss/dev": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@ark-ui/react": "^4.4.0",
    "@dnd-kit/core": "^6.1.0"
  },
  "devDependencies": {
    "@cerberus-design/configs": "workspace:*",
    "@cerberus-design/styled-system": "workspace:*",
    "@microsoft/api-extractor": "^7.46.2",
    "@types/react": "^18",
    "@types/react-dom": "^19",
    "react": "^18",
    "react-dom": "^19",
    "tsup": "^8.1.0"
  },
  "publishConfig": {
    "access": "public",
    "types": "build/legacy/_tsup-dts-rollup.d.ts",
    "main": "build/legacy/index.cjs",
    "module": "build/modern/index.js",
    "exports": {
      ".": {
        "import": {
          "types": "./build/modern/_tsup-dts-rollup.d.ts",
          "default": "./build/modern/index.js"
        },
        "require": {
          "types": "./build/legacy/_tsup-dts-rollup.d.cts",
          "default": "./build/legacy/index.cjs"
        }
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        ".": [
          "build/legacy/_tsup-dts-rollup.d.cts"
        ]
      }
    }
  },
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "keywords": [
    "panda-css",
    "omni-federal",
    "design-system",
    "css",
    "react",
    "ui",
    "ui library",
    "panda-css react",
    "digital university",
    "du design system"
  ],
  "author": "developers@omnifederal.com",
  "license": "ISC"
}
